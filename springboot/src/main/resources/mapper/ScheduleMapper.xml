<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot.system.mapper.ScheduleMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.springboot.system.entity.ScheduleEntity">
        <id column="schedule_id" property="id" />
        <result column="schedule_team" property="team" />
        <result column="schedule_time" property="time" />
        <result column="schedule_week" property="week" />
        <result column="schedule_list" property="list" />
        <result column="create_by" property="createBy" />
        <result column="create_time" property="createTime" />
        <result column="update_by" property="updateBy" />
        <result column="update_time" property="updateTime" />
        <result column="undock" property="undock" />
        <result column="state" property="state" />
        <result column="remark" property="remark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        schedule_id, schedule_team, schedule_time, schedule_week, schedule_list, create_by, create_time, update_by, update_time, undock, state, remark
    </sql>

</mapper>
