<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot.system.mapper.TableMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.springboot.system.entity.TableEntity">
        <id column="table_id" property="id"/>
        <result column="table_time" property="time"/>
        <result column="table_week" property="week"/>
        <result column="table_list" property="list"/>
        <result column="table_teacher" property="teacher"/>
        <result column="table_classroom" property="classroom"/>
        <result column="create_by" property="createBy"/>
        <result column="create_time" property="createTime"/>
        <result column="update_by" property="updateBy"/>
        <result column="update_time" property="updateTime"/>
        <result column="undock" property="undock"/>
        <result column="state" property="state"/>
        <result column="remark" property="remark"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        table_id, table_time, table_week, table_list,table_teacher,table_classroom, create_by, create_time, update_by, update_time, undock, state, remark
    </sql>

</mapper>
